<h2>{{title}}</h2>
<table id="board">
	{{#each board}}
			<tr>
			  {{#each this}}
				<td>
				  {{#this}}
				    <button id="square-{{x}}-{{y}}" class="{{#lit}}lit{{/lit}}{{^lit}}not-lit{{/lit}}">
				      {{#lit}}lit{{/lit}}
				      {{^lit}}not lit{{/lit}}
				    </button>
				  {{/this}}
				</td>
				{{/each}}
			</tr>
	{{/each}}
</table>